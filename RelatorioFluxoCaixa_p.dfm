  object qFluxo: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftDate
        Name = 'dataI'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'select credito, debito, vencimento, descricao'
      'from ('
      ''
      
        'select cp.valor as debito, 0 as credito, cp.vencimento, cp.descr' +
        'icao'
      'from ctas_pagar cp'
      'where cp.liquidado <> 1 or (cp.liquidado is null)'
      ''
      'union'
      ''
      
        'select 0 as debito, cr.valor as credito, cr.vencimento, cr.tipo_' +
        'venda'
      'from ctas_receber cr'
      'where cr.liquidado <> 1 or (cr.liquidado is null)'
      ''
      ')'
      'where vencimento = :dataI '
      ''
      'order by vencimento')
    SQLConnection = DMPrincipal.SQLCon
    Left = 32
    Top = 247
  end
